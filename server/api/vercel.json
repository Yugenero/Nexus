{
  "version": 2,
  "builds": [
    { "src": "api.js", "use": "@vercel/node" },
    { "src": "postRoutesApi.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/api.js", "dest": "api.js" },
    { "src": "/api/postRoutesApi.js", "dest": "postRoutesApi.js" },
    { "src": "/api/(.*)", "dest": "api.js" },
    { "src": "/(.*)", "dest": "/src/$1" }
  ]
}
