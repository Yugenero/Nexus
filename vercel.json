{
	"version": 2,
	"builds": [
	  {
		"src": "server/api/**/*.js",
		"use": "@vercel/node"
	  },
	  {
		"src": "package.json",
		"use": "@vercel/static-build"
	  }
	],
	"routes": [
	  {
		"src": "/api/(.*)",
		"dest": "server/api/$1.js"
	  },
	  {
		"src": "/(.*)",
		"dest": "/index.html"
	  }
	]
  }
  