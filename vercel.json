{
	"version": 2,
	"builds": [
	  {
		"src": "server/api/**/*.js",
		"use": "@vercel/node"
	  },
	  {
		"src": "src/client-code/**/*",
		"use": "@vercel/static-build",
		"config": {
		  "distDir": "build"
		}
	  }
	],
	"routes": [
	  {
		"src": "/api/(.*)",
		"dest": "server/api/$1.js"
	  },
	  {
		"src": "/(.*)",
		"dest": "/index.html"
	  }
	]
  }
  